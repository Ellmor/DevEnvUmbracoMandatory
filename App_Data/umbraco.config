    <articlesMain id="1069" key="24251b22-c99b-433c-9a6c-a147aaa4699e" parentID="1080" level="2" creatorID="0" sortOrder="2" createDate="2016-02-12T09:54:46" updateDate="2016-03-28T23:34:26" nodeName="Articles" urlName="articles" path="-1,1080,1069" isDoc="" nodeType="1057" creatorName="Piotr S" writerName="Piotr S" writerID="0" template="1056" nodeTypeAlias="articlesMain">
